{
  "name": "mydiary-backend",
  "version": "1.0.0",
  "description": "backend for mydiary app",
  "main": "src/index.js",
  "scripts": {
    "db:migrate:make": "knex --knexfile src/database/knexfile.ts -x ts migrate:make",
    "db:migrate:latest": "knex --knexfile src/database/knexfile.ts migrate:latest",
    "db:migrate:rollback": "knex --knexfile src/database/knexfile.ts migrate:rollback",
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevoese/myDiary-backend.git"
  },
  "author": "kelvin esegbona",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kevoese/myDiary-backend/issues"
  },
  "homepage": "https://github.com/kevoese/myDiary-backend#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "knex": "^0.20.3",
    "objection": "^2.0.2",
    "pg": "^7.14.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^12.12.14",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  }
}
